import heapq

graph = {
    'A': {'B':1, 'C':4},
    'B': {'D':3, 'E':5},
    'C': {'F':2},
    'D': {'F':1, 'E':1},
    'E': {'F':2},
    'F': {}
}

# Heuristic values (straight-line estimates to Goal F)
h = {'A':6, 'B':4, 'C':2, 'D':1, 'E':2, 'F':0}

def astar(start, goal):
    pq = [(h[start], 0, start, [start])]  # (f = g+h, g, node, path)
    visited_order = []

    while pq:
        f, g, node, path = heapq.heappop(pq)
        visited_order.append(node)

        if node == goal:
            return visited_order, path, g

        for nxt, cost in graph[node].items():
            heapq.heappush(pq, (g + cost + h[nxt], g + cost, nxt, path + [nxt]))

    return None

visited, path, total = astar('A', 'F')
print("Node Expansion Order:", visited)
print("Optimal Path:", " -> ".join(path))
print("Total Cost:", total)
